<?xml version="1.0" encoding="UTF-8" ?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd">
	<virtualType name="Applab\Subscription\Model\ResourceModel\SubscriptionData\Grid\Collection" type="Magento\Framework\View\Element\UiComponent\DataProvider\SearchResult">
        <arguments>
            <argument name="mainTable" xsi:type="string">applab_subscriptions</argument>
            <argument name="resourceModel" xsi:type="string">Applab\Subscription\Model\ResourceModel\SubscriptionData</argument>
        </arguments>
    </virtualType>
    <type name="Magento\Framework\View\Element\UiComponent\DataProvider\CollectionFactory">
        <arguments>
            <argument name="collections" xsi:type="array">
                <item name="subscription_listing_data_source" xsi:type="string">Applab\Subscription\Model\ResourceModel\SubscriptionData\Grid\Collection</item>
            </argument>
        </arguments>
    </type>

    <type name="Magento\Quote\Model\Quote\Item\ToOrderItem">
        <plugin name="delivery_datas_attribute_quote_to_order_item" type="Applab\Subscription\Plugin\SetCustomDataToOrderItem"/>
    </type>
</config>